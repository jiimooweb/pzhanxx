  border: 3rpx solid #fff;
  color: #fff;
  font-size: 28rpx;
  font-weight: 500;
}

.ad-wapper {
  width: 100%;
  box-sizing: border-box;
}

.ad-tips {
  font-size: 26rpx;
  color:#606266;
  line-height: 55rpx;
  padding:0 5rpx;
  text-align: center;
}

.toady-btn {
  display: flex;
  flex-direction: column;
