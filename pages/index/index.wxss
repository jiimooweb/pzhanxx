}

.comment-center {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  margin-bottom: 50rpx;
}

.toady-btn image {
  width: 45rpx;
  height:45rpx;
  margin-top: 8rpx;
  margin-bottom: 5rpx;
}

.toady-btn text {
  font-size: 20rpx;
  color: #fff;
}

.to-top {
