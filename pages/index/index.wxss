@import "../../utils/common.wxss";

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.suspension-btn.iphonex {
  bottom: 30rpx;
}

scroll-view {
  height: calc(100% - 120rpx)
}

.swiper {
  height: 405rpx;
  overflow: visible;
}

.swiper swiper-item {
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: visible;
}

.swiper button {
  width: 100%;
  height: 345rpx;
  transform: scale(0.9246, 0.9246) translateZ(0);
  transition: transform 0.5s, -webkit-transform 0.5s;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 5rpx 5rpx 25rpx 0 rgba(0, 0, 0, 0.1);
}

.swiper button.ori {
  transform: scale(1, 1) translateZ(0);
}

.swiper-image {
  width: 100%;
  height: 100%;
  display: block;
}

.swiper text {
  width: 114rpx;
  height: 40rpx;
  background: #fff;
  position: absolute;
  font-size: 20rpx;
  color: #3399cb;
  text-align: center;
  line-height: 40rpx;
  right: 0;
  top: 20rpx;
  border-bottom-left-radius: 30rpx;
  border-top-left-radius: 30rpx;
  box-shadow: 1rpx 1rpx 6rpx 1rpx rgba(51, 153, 203, 0.31);
}

.hot-album {
  height: 350rpx;
}

.hot-album, .hot-dynamic {
  padding: 34rpx 16rpx 0 16rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  position: relative;
}

.album-view .hot-btn {
  height: 250rpx;
}

.hot-btn image{
  height: 250rpx;
}

.recommend-pic {
  padding: 34rpx 16rpx 0 16rpx;
  margin-bottom: 10rpx;
  overflow: hidden;
  position: relative;
}

.hot-album .album-view {
  display: flex;
  justify-content: space-between;
  margin-top: 34rpx;
}

.hot-album .album-view text {
  position: absolute;
  width: 64rpx;
  height: 34rpx;
  border-top-left-radius: 13rpx;
  border-bottom-left-radius: 13rpx;
  background-color: #239efe;
  right: 0rpx;
  top: 10rpx;
  font-size: 20rpx;
  color: #fff;
  line-height: 34rpx;
}

.hot-album .album-view view {
  font-size: 28rpx;
  color: #323232;
  line-height: 88rpx;
  width: 354rpx;
  text-align: left;
  font-weight: 500;
}

.hot-album .album-view text.act {
  background-color: #f49241;
}

.hot-album .album-view image {
  width: 354rpx;
  height: 188rpx;
  display: block;
  border-radius: 18rpx;
}

.dynamic-view {
  margin-top: 35rpx;
}

.dynamic-view-body {
  display: flex;
  padding-bottom: 50rpx;
}

.dynamic-view .left {
  width: 216rpx;
  height: 288rpx;
  display: block;
  margin-right: 50rpx;
}

.dynamic-view .right {
  width: 418rpx;
  position: relative;
}

.dynamic-view .userInfo {
  display: flex;
  height: 80rpx;
  align-items: center;
  font-size: 28rpx;
  color: #4c4c4c;
  font-weight: 500;
}

.dynamic-view .userInfo image {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  background-image: url('https://minibizhi.313515.com/svg/default.svg');
  background-size: cover;
}

.dynamic-view .reply-word {
  font-size: 30rpx;
  color: #949494;
  line-height: 48rpx;
  text-align: left;
  margin-top: 20rpx;
  height: 96rpx;
}

.hot-dynamic .module-title {
  background: none;
}

.hot-dynamic .topicLabel {
  position: absolute;
  right: -16rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #fff;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  background: #239efe;
  text-align: center;
  padding: 0 14rpx;
}

.hot-dynamic .topicLabel.act {
  background: #f5a623;
}

.hot-dynamic .topicLabel.brandAct {
  background: #ff6d7f;
}

.hot-dynamic .toTopicBtn {
  position: absolute;
  bottom: 0;
  left: 0;
  max-width: 418rpx;
}

.dynamic-wallpaper {
  padding: 34rpx 16rpx 0 16rpx;
  margin-bottom: 20rpx;
}

.dynamic-wallpaper .dynamic-wallpaper-view {
  display: flex;
  justify-content: space-between;
  padding-bottom: 40rpx;
  margin-top: 34rpx;
}

.dynamic-wallpaper .dynamic-wallpaper-view image {
  width: 230rpx;
  height: 410rpx;
}

.change-data {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 30rpx;
  padding-bottom: 38rpx;
  padding-top: 10rpx;
  border-bottom: 1rpx solid #e6e6e6;
  box-sizing: content-box;
}

.change-data image {
  width: 34rpx;
  height: 28rpx;
  display: block;
}

.change-data text {
  font-size: 26rpx;
  color: #4c4c4c;
  margin-left: 15rpx;
  display: block;
}

.author-column {
  padding: 34rpx 16rpx;
}

.author-column .module-title text {
  display: block;
  width: 108rpx;
  height: 30rpx;
  font-size: 20rpx;
  line-height: 30rpx;
  color: #fff;
  background: #ff878c;
  border-radius: 15rpx;
  text-align: center;
  position: absolute;
  top: 1rpx;
  left: 156rpx;
}

.author-column-view {
  display: flex;
  justify-content: space-between;
  position: relative;
  align-items: flex-end;
  padding-bottom: 50rpx;
  margin-top: 35rpx;
  height: 498rpx;
}

.ipx .author-column-view {
  height: 606rpx;
}

.author-column-view .img-btn {
  width: 140rpx;
  height: 249rpx;
}

.ipx .author-column-view .img-btn {
  height: 303rpx;
}

.author-column-view .img-btn:first-child {
  width: 280rpx;
  height: 100%;
}

.author-column-view .img-btn image:first-child {
  width: 100%;
  height: 100%;
}

.author-column-view .img-btn .buy-flag {
  width: 70rpx;
  height: auto;
  position: absolute;
  top: 8rpx;
  left: 6rpx;
  background: none;
}

.author-column-view .img-btn:first-child .buy-flag {
  width: 104rpx;
}

.author-column-view .author {
  position: absolute;
  right: 0;
  top: 0;
  width: 432rpx;
}

.author-column-view .author .top {
  height: 96rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  overflow: visible;
}

.author-column-view .author .top .top-left text:first-child {
  font-size: 28rpx;
  color: #4c4c4c;
  line-height: 40rpx;
  display: block;
  font-weight: 500;
}

.author-column-view .author .top .top-left text:last-child {
  font-size: 22rpx;
  color: #707070;
  line-height: 30rpx;
  margin-top: 10rpx;
  display: block;
}

.author-column-view .author .top .top-right {
  width: 96rpx;
  height: 96rpx;
  margin-left: 20rpx;
  position: relative;
}

.author-column-view .author .top .top-right image:first-child {
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 58rpx;
  box-shadow: 0px 0px 2rpx 1rpx rgba(0, 0, 0, 0.3);
}

.author-column-view .author .top .top-right image:last-child {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  background-image: linear-gradient(-159deg, #ffd068 0%, #ffae1b 74%, #ffa300 100%);
  border: 1rpx solid #fff;
  border-radius: 50%;
}

.author-column-view .author .bottom {
  display: flex;
  align-items: center;
  height: 140rpx;
}

.ipx .author-column-view .author .bottom {
  height: 180rpx;
}

.author-column-view .author .bottom view {
  padding-left: 62rpx;
  padding-right: 6rpx;
  position: relative;
  flex: 1;
}

.author-column-view .author .bottom text {
  font-size: 28rpx;
  color: #949494;
  line-height: 40rpx;
  overflow: hidden;
  max-height: 80rpx;
  width: 100%;
  white-space: pre-wrap;
}

.top-nav-warp {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
  flex-wrap: wrap;
}

.top-nav-warp button {
  background-color: transparent;
  flex: 1;
}

.top-nav-warp view {
  position: relative;
  height: 110rpx;
  width: 20%;
  padding: 24rpx 0;
}

.top-nav-warp .icon {
  width: 64rpx;
  height: 64rpx;
  display: block;
  margin: 0 auto;
}

.top-nav-warp text {
  font-size: 26rpx;
  color: #4c4c4c;
  line-height: 1.2;
  display: block;
  margin-top: 14rpx;
}

.version-tip {
  position: absolute;
  z-index: 1000;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.4);
  display: flex;
  justify-content: center;
  align-items: center;
}

.version-tip .version {
  width: 528rpx;
  height: 630rpx;
}

.version-tip view {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: 185rpx;
  margin-top: -378rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.version-tip view .version-close {
  width: 58rpx;
  height: 58rpx;
}

.data-list-box {
  min-height: 960rpx;
}

.scroll-nav-fixed {
  position: absolute;
  width: 100%;
  height: 82rpx;
  z-index: 1;
}

.scroll-nav-wrap {
  height: 82rpx;
}

.activity-view {
  position: relative;
  margin-top: 36rpx;
}

.activity-view .bg {
  width: 718rpx;
  height: 380rpx;
  display: block;
  margin: 0 auto;
}

.activity-view text {
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  transform: translateY(-50%);
  text-shadow: 0 2rpx 6rpx rgba(0, 0, 0, 0.29);
  font-weight: bold;
  text-align: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #fff;
}

.activity-view .brand {
  display: flex;
  position: absolute;
  top: 26rpx;
  left: 30rpx;
  align-items: center;
}

.activity-view .brand image {
  display: block;
  width: 48rpx;
  height: 48rpx;
  border-radius: 13rpx;
}

.activity-view .brand image:nth-child(2) {
  width: 14rpx;
  height: 14rpx;
  border-radius: 0;
  margin: 0 12rpx;
}

.row2Image .data-list-box image {
  width: 354rpx;
  height: 630rpx;
}


.loading-tip {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  color:#909399;
  height:100rpx;
  width: 100%;
  text-align: center;
  margin-bottom: 20rpx;
}





.ipx .row2Image .data-list-box image {
  height: 766.5rpx;
}

.search-wrap {
  transition: all 0.3s;
}

.slideUp {
  transform: translate3d(0, -100%, 0);
}

.crossFixed.scroll-nav-fixed {
