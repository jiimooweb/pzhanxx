  top: 74rpx;
}

.listContainer {
  margin-top: 34rpx;
}

.addTip {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  z-index: 100;
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
  
}

.addTip image{
  margin-top:30rpx;
  height: 100%;
  width: 100%;
}

.addTip .know{
  position: absolute;
  bottom: 150rpx;
