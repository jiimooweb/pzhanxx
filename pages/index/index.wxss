} */

.fullAd image {
    width: 100%;
    height: 100%;
}

.confirm-btn {
    z-index: 135;
}

.confirm-btn {
    display: flex;
    flex-direction: column;
    position: fixed;
    bottom: 60rpx;
    right: 40rpx;
    border-radius: 30rpx;
    background-color: rgba(0, 0, 0, 0.5);
    text-align: center;
    align-items: center;
    padding: 8rpx 25rpx;
    z-index: 32;
    color: #fff;
    font-size: 30rpx;
}

.mask {
    position: absolute;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 101;
}

.ad-title {
    font-size: 26rpx;
    color: #AFAFAF;
    display: flex;
    line-height: 55rpx;
    width: 100%;
    align-items: center;
    justify-content: center;
}
.indexImg{
    width: 100%;
    height: 220rpx;
}
